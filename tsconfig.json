{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "baseUrl": "src",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "ts-build",
    "jsx": "react",
    "types": [
      "react",
      "redux",
      "core-js"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
